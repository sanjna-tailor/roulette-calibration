		"format": "npx eslint src/ --ext ts --fix"
	},
	"types": "dist/index.d.ts",
	"main": "dist/cjs/areax-pixi-core.js",
	"module": "dist/esm/areax-pixi-core.js",
	"license": "UNLICENSED",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18",
		"npm": ">=8",
		"yarn": ">=1"
	},
	"dependencies": {
		"@pixi/particle-emitter": "5.0.8",
		"@types/howler": "2.2.4",
		"eventemitter3": "4.0.7",
		"gsap": "3.12.2",
		"howler": "2.2.3",
		"pixi-projection": "1.0.0",
		"pixi-spine": "4.0.4",
		"pixi.js": "7.3.3"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-terser": "0.4.4",
		"@types/node": "^18.0.0",
		"@types/offscreencanvas": "2019.7.0",
		"@typescript-eslint/eslint-plugin": "6.19.1",
